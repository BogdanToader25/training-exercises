
import signal
import os
import threading
import time
import sys


def thread_job():
    time.sleep(1)
    print("!!! WARNING ENTERED IN AN INFINITE LOOP !!!")
    
    os.kill(os.getpid(), signal.SIGINT)



fail_safe = threading.Thread(target=thread_job, daemon=True)
fail_safe.start()
# print on a separate line using a while statement
# all the numbers in the list
a = [8, 3, 11]
print("merge")